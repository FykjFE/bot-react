{
  "name": "bot-react",
  "version": "1.0.0",
  "description": "react 脚手架",
  "keywords": [
    "react",
    "cli"
  ],
  "homepage": "https://github.com/FykjFE/bot-react#readme",
  "bugs": {
    "url": "https://github.com/FykjFE/bot-react/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FykjFE/bot-react.git"
  },
  "license": "MIT",
  "author": "zzf",
  "main": "src/index.tsx",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack  --config config/webpack.prod.config.js",
    "build:test": "cross-env NODE_ENV=test webpack  --config config/webpack.prod.config.js",
    "format": "npm run prettier && npm run lint ",
    "lint": "eslint src/**/*.{tsx,ts}",
    "test": "sh script/lint.sh",
    "lint:fix": "eslint --fix src/**/*.{tsx,ts,js}",
    "prettier": "prettier --write src/**/*.{tsx,ts}",
    "lint:style": "stylelint --config .stylelintrc.js --fix src/**/*.scss",
    "start": "cross-env NODE_ENV=development webpack-dev-server --config config/webpack.dev.config.js --colors"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{scss}": [
      "stylelint --config .stylelintrc.js --fix"
    ],
    "src/**/*.{ts,tsx,js,json,scss,md}": [
      "prettier --write"
    ],
    "src/**/*.{ts,tsx,js}": [
      "eslint --config .eslintrc.json"
    ]
  },
  "dependencies": {
    "antd": "^4.5.4",
    "axios": "^0.20.0",
    "dayjs": "^1.8.35",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@types/node": "^14.10.1",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@types/react-redux": "^7.1.9",
    "@types/react-router-dom": "^5.1.5",
    "@types/webpack-env": "^1.15.2",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "antd-dayjs-webpack-plugin": "^1.0.1",
    "babel-loader": "^8.1.0",
    "babel-plugin-import": "^1.13.0",
    "clean-webpack-plugin": "^3.0 .0",
    "copy-webpack-plugin": "^6.1.0",
    "cross-env": "^7.0.2",
    "css-loader": "^4.2.1",
    "cssnano": "^4.1.10",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.2",
    "file-loader": "^6.1.0",
    "fork-ts-checker-webpack-plugin": "^5.2.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "hard-source-webpack-plugin": "^0.13.1",
    "html-webpack-plugin": "^4.3.0",
    "husky": "^4.3.0",
    "less": "^3.12.2",
    "less-loader": "^7.0.1",
    "lint-staged": "^10.3.0",
    "mini-css-extract-plugin": "^0.11.2",
    "node-sass": "^4.14.1",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "prettier": "^2.1.1",
    "sass-loader": "^9.0.3",
    "style-loader": "^1.2.1",
    "stylelint": "^13.7.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.3.0",
    "stylelint-order": "^4.1.0",
    "terser-webpack-plugin": "^4.2.0",
    "thread-loader": "^3.0.0",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "typescript": "^4.0.2",
    "url-loader": "^4.1.0",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.1.2",
    "webpackbar": "^4.0.0"
  }
}
